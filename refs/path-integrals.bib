@article{weber_master_2017,
	title = {Master equations and the theory of stochastic path integrals},
	volume = {80},
	issn = {0034-4885, 1361-6633},
	url = {http://stacks.iop.org/0034-4885/80/i=4/a=046601?key=crossref.02abaf744081951aaaca3bafec0e1284},
	doi = {10.1088/1361-6633/aa5ae2},
	abstract = {This review provides a pedagogic and self-contained introduction to master equations and to their representation by path integrals. Since the 1930s, master equations have served as a fundamental tool to understand the role of fluctuations in complex biological, chemical, and physical systems. Despite their simple appearance, analyses of master equations most often rely on low-noise approximations such as the Kramers{\textendash}Moyal or the system size expansion, or require ad-hoc closure schemes for the derivation of low-order moment equations. We focus on numerical and analytical methods going beyond the low-noise limit and provide a unified framework for the study of master equations. After deriving the forward and backward master equations from the Chapman{\textendash}Kolmogorov equation, we show how the two master equations can be cast into either of four linear partial differential equations (PDEs). Three of these PDEs are discussed in detail. The first PDE governs the time evolution of a generalized probability generating function whose basis depends on the stochastic process under consideration. Spectral methods, WKB approximations, and a variational approach have been proposed for the analysis of the PDE. The second PDE is novel and is obeyed by a distribution that is marginalized over an initial state. It proves useful for the computation of mean extinction times. The third PDE describes the time evolution of a {\textquoteleft}generating functional{\textquoteright}, which generalizes the so-called Poisson representation. Subsequently, the solutions of the PDEs are expressed in terms of two path integrals: a {\textquoteleft}forward{\textquoteright} and a {\textquoteleft}backward{\textquoteright} path integral. Combined with inverse transformations, one obtains two distinct path integral representations of the conditional probability distribution solving the master equations. We exemplify both path integrals in analysing elementary chemical reactions. Moreover, we show how a wellknown path integral representation of averaged observables can be recovered from them.},
	language = {en},
	number = {4},
	urldate = {2018-11-30},
	journal = {Reports on Progress in Physics},
	author = {Weber, Markus F and Frey, Erwin},
	month = apr,
	year = {2017},
	pages = {046601},
	file = {Weber and Frey - 2017 - Master equations and the theory of stochastic path.pdf:/Users/jkappler/Zotero/storage/QSPITCS5/Weber and Frey - 2017 - Master equations and the theory of stochastic path.pdf:application/pdf},
}

