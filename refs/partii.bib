@article{sackBiologicalTissueMechanics2016,
  title = {Biological Tissue Mechanics with Fibres Modelled as One-Dimensional {{Cosserat}} Continua. {{Applications}} to Cardiac Tissue},
  author = {Sack, K. L. and Skatulla, S. and Sansour, C.},
  year = {2016},
  month = mar,
  journal = {International Journal of Solids and Structures},
  volume = {81},
