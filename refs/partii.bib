@article{ausasFiniteElementMethod2022,
  title = {A Finite Element Method for Simulating Soft Active Non-Shearable Rods Immersed in Generalized {{Newtonian}} Fluids},
  author = {Ausas, Roberto Federico and Gebhardt, Cristian Guillermo and Buscaglia, Gustavo Carlos},
  year = {2022},
